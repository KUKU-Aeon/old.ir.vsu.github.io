			<script src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/modernizr/3/modernizr-custom.js" type="text/javascript"></script>
			<script src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/modernizr/modernizr.custom.min.js" type="text/javascript"></script>
