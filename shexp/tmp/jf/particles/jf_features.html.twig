{% block stylesheets %}
	{% if particle.jf_iewarning %}
											<style type="text/css">
												#jf_iewarning {
													z-index: 99999;
													position: fixed;
													top: 0;
													left: 0;
													right: 0;
													bottom: 0;
													background: #f5f5f5;
													text-align: center;
													padding: 10% 0;
												}
												#jf_iewarning h4 {margin:10px 0 35px}
											</style>
	{% endif %}
	{% for item_syntax in particle.jf_syntax %}
		{% if item_syntax.prism %}			<link rel="stylesheet" href="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/jf_syntax/prism.min.css" type="text/css" />{% endif %}
	{% endfor %}
	{% for item_stickyHeader in particle.stickyHeader %}
		{% if item_stickyHeader.enable %}
											<style type="text/css">
												body #g-header {
													position: fixed;
													right: 0;
													left: 0;
													top: 0;
													z-index: 992;
													-webkit-transition: all 0.3s ease;
													-khtml-transition: all 0.3s ease;
													-moz-transition: all 0.3s ease;
													-ms-transition: all 0.3s ease;
													-o-transition: all 0.3s ease;
													transition: all 0.3s ease;
													-webkit-transform: translate3d(0,0,0);
													-khtml-transform: translate3d(0,0,0);
													-moz-transform: translate3d(0,0,0);
													-ms-transform: translate3d(0,0,0);
													-o-transform: translate3d(0,0,0);
													transform: translate3d(0,0,0);
												}
												#g-header.slideUp {
													-webkit-transform: translate3d(0,-102%,0) !important;
													-khtml-transform: translate3d(0,-102%,0) !important;
													-moz-transform: translate3d(0,-102%,0) !important;
													-ms-transform: translate3d(0,-102%,0) !important;
													-o-transform: translate3d(0,-102%,0) !important;
													transform: translate3d(0,-102%,0) !important;
												}
												{% if item_stickyHeader.slideup == false %}
													#g-header.headroom--not-top.slideUp {
														-webkit-transform: translate3d(0,0,0);
														-khtml-transform: translate3d(0,0,0);
														-moz-transform: translate3d(0,0,0);
														-ms-transform: translate3d(0,0,0);
														-o-transform: translate3d(0,0,0);
														transform: translate3d(0,0,0);
													}
												{% endif %}

												#g-header.headroom--not-top .g-main-nav .g-toplevel > li > .g-menu-item-container,
												#g-header.headroom--not-top .jf_panel_tools li .jf_panel_btn {
													padding: {{ item_stickyHeader.menu_padding|raw }} 1rem;
												}
												#g-header.headroom--not-top .jf_canvas_logo {
													height: {{ item_stickyHeader.logo_size|raw }} !important;
												}
												#g-header.headroom--not-top .jf_canvas_logo a {
													-webkit-transform: scale({{ item_stickyHeader.logo_scale|raw }}) !important;
													-moz-transform: scale({{ item_stickyHeader.logo_scale|raw }}) !important;
													-ms-transform: scale({{ item_stickyHeader.logo_scale|raw }}) !important;
													-o-transform: scale({{ item_stickyHeader.logo_scale|raw }}) !important;
													transform: scale({{ item_stickyHeader.logo_scale|raw }}) !important;
												}
												#g-header .g-main-nav .g-toplevel > li > .g-menu-item-container,
												#g-header .jf_panel_tools li .jf_panel_btn,
												#g-header .jf_canvas_logo,
												#g-header .jf_canvas_logo a {
													-webkit-transition: padding, height, width, 0.3s ease;
													-khtml-transition: padding, height, width, 0.3s ease;
													-moz-transition: padding, height, width, 0.3s ease;
													-ms-transition: padding, height, width, 0.3s ease;
													-o-transition: padding, height, width, 0.3s ease;
													transition: padding, height, width, 0.3s ease;
												}
												
												@media only screen and (max-width: 414px){
												  #g-header .jf_canvas_logo_wrapper {
													margin: 0 !important;
												  }
												  .jf_canvas_logo {
													width: 100% !important;
												  }
												  .jf_canvas_logo a {
													transform-origin: center top !important;
													margin: 0 auto;
												  }
												  #g-header.headroom--not-top .jf_canvas_logo {
													height: {{ item_stickyHeader.logo_size|raw }} !important;
												  }
												  .jf_canvas_logo_outer {
													display: none;
												  }
												}
												@media only screen and (min-width:415px) and (max-width: 768px){
												  #g-header .jf_canvas_logo_wrapper {
													margin: 0 0 0 15px !important;
												  }
												  #g-header .jf_canvas_logo_outer {
													height: 0 !important;
												  }
												}
												@media only screen and (min-width:415px) and (max-width: 480px){
												  #g-header .jf_canvas_logo_wrapper {
													margin: 0 10px !important;
												  }
												  #g-header.headroom--not-top .jf_canvas_logo_wrapper {
													margin: 0 20px !important;
												  }
												}
												{% if item_stickyHeader.homepage == 'transparent' %}
													#g-header.headroom--top {
														background-color: rgba(0, 0, 0, 0);
														-webkit-box-shadow: none;
														-khtml-box-shadow: none;
														-moz-box-shadow: none;
														-ms-box-shadow: none;
														-o-box-shadow: none;
														box-shadow: none;
														z-index: 991;
													}
													.jf_head_set {
														height: 0 !important;
													}
												{% elseif item_stickyHeader.homepage == 'gradient' %}
													#g-header.headroom--top {
														background-color: rgba(0, 0, 0, 0);
														-webkit-box-shadow: none;
														-khtml-box-shadow: none;
														-moz-box-shadow: none;
														-ms-box-shadow: none;
														-o-box-shadow: none;
														box-shadow: none;
														z-index: 991;
													}
													#g-header.headroom--top:after {
														content: '';
														position: absolute;
														top: -5px;
														left: 0;
														width: 100%;
														height: 120%;
														opacity: 0.7;
														background: -webkit-linear-gradient(to bottom, #000 0%, rgba(0, 0, 0, 0) 100%);
														background: -moz-linear-gradient(to bottom, #000 0%, rgba(0, 0, 0, 0) 100%);
														background: -ms-linear-gradient(to bottom, #000 0%, rgba(0, 0, 0, 0) 100%);
														background: -o-linear-gradient(to bottom, #000 0%, rgba(0, 0, 0, 0) 100%);
														background: linear-gradient(to bottom, #000 0%, rgba(0, 0, 0, 0) 100%);
														z-index: -1;
													}
													.jf_head_set {
														height: 0 !important;
													}
													#g-header.headroom--top .g-main-nav .g-toplevel > li > .g-menu-item-container,
													#g-header.headroom--top .jf_panel_tools > li .jf_panel_btn {
														color: #fff;
													}
												{% endif %}
											</style>
		{% endif %}
	{% endfor %}
	
	{% for item_jf_preloader in particle.jf_preloader %}
		{% if item_jf_preloader.enable %}	
											{#<link rel="stylesheet" href="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/jf_preloader/jf_animsition.css" type="text/css" />#}
											<link rel="stylesheet" href="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/jf_preloader/jf_pace.min.css" type="text/css" />
											<style>
												#g-page-surround{opacity:0}
												body .pace .pace-progress{background:{{ item_jf_preloader.progressColor|e }}}
												body .pace .pace-progress-inner{
													-webkit-box-shadow:0 0 10px {{ item_jf_preloader.progressColor|e }},0 0 5px {{ item_jf_preloader.progressColor|e }};
													-moz-box-shadow:0 0 10px {{ item_jf_preloader.progressColor|e }},0 0 5px {{ item_jf_preloader.progressColor|e }};
													box-shadow:0 0 10px {{ item_jf_preloader.progressColor|e }},0 0 5px {{ item_jf_preloader.progressColor|e }};
												}
											</style>
		{% endif %}
	{% endfor %}
	
	{% for item_jf_scrollTop in particle.jf_scrollTop %}
		{% if item_jf_scrollTop.enable %}
											<style type="text/css">
												#jf_scrollTop {
													z-index: 99;
													position: fixed;
													bottom: -50px;
													right: 30px;
													display: block;
													text-align: center;
													color: {{ item_jf_scrollTop.iconColor|e }};
													width: 60px;
													height: 60px;
													background-color: {{ item_jf_scrollTop.bgColor|e }};
													cursor: pointer;
													-webkit-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
													-khtml-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
													-moz-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
													-ms-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
													-o-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
													box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
													-webkit-border-radius: 50%;
													-khtml-border-radius: 50%;
													-moz-border-radius: 50%;
													-ms-border-radius: 50%;
													-o-border-radius: 50%;
													border-radius: 50%;
													/*opacity: 0;*/
													-webkit-transition: all .2s ease-out,bottom .3s linear;
													-moz-transition: all .2s ease-out,bottom .3s linear;
													transition: all .2s ease-out,bottom .3s linear;
													-webkit-transform: scale(0);
													-moz-transform: scale(0);
													-ms-transform: scale(0);
													transform: scale(0);
												}
												#jf_scrollTop.jf_show {
												    /*opacity: 1;*/
													bottom: 30px;
													-webkit-transition: all .2s ease-out,bottom .3s cubic-bezier(0.1,.6,.27,1.26);
													-moz-transition: all .2s ease-out,bottom .3s cubic-bezier(0.1,.6,.27,1.26);
													transition: all .2s ease-out,bottom .3s cubic-bezier(0.1,.6,.27,1.26);
													-webkit-transform: scale(1);
													-moz-transform: scale(1);
													-ms-transform: scale(1);
													transform: scale(1);
												}
												#jf_scrollTop i {
													-webkit-transform: rotate(-90deg);
													-khtml-transform: rotate(-90deg);
													-moz-transform: rotate(-90deg);
													-ms-transform: rotate(-90deg);
													-o-transform: rotate(-90deg);
													transform: rotate(-90deg);
													font-size: 32px;
													line-height: 60px;
												}
											</style>
		{% endif %}
	{% endfor %}
	
	
{% endblock %}




