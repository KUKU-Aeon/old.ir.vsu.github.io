											{% assets in 'head' with { priority: 1 } %}
												<script src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/jf_preloader/jf_pace.min.js" type="text/javascript"></script>
											{% endassets -%}
											
											<script type="text/javascript">
												jQuery(document).ready(function($){
													var jf_preloader_tags	= $("#g-page-surround");
														
													Pace.on("hide", function(){
														// alert("done");
														jf_preloader_tags.addClass("jf_pace_moveUpWrapper").animate({opacity:1});
															$(".pace-progress").hide(0);
														setTimeout(function() {
															$(".pace").addClass("jf_pace_loaded");
															jf_preloader_tags.removeClass("jf_pace_moveUpWrapper");
														}, 500);
													});
													$(window).bind('beforeunload', function(){
														jf_preloader_tags.animate({opacity:0});
														$(".pace").addClass("jf_pace_leave").removeClass("pace-inactive");
														// alert("leaving");
													});
													{#// click event for links which has no href="javascript" and href="#"
														function redirectPage(){
															window.location = linkLocation;
														}
														jf_preloader_notlink		= "[href^=javascript:void(0)],[href^=javascript:void(0);],[href^=#],[target^=_blank],[onclick^=window.open],[data-size],.jf_venobox,.chzn-single,[href*=javascript: void( 0 )],[href*=.jpg]";

														$('a[href]').click(function(event){
															if(!$(this).is(jf_preloader_notlink) && $(this).is('[href*="#"]')){
																"use strict";
																var url 		= document.location.href;
																var pathname 	= $(this).attr('href').split('#')[0];
																var hash 		= $(this).attr('href').split('#')[1];
																// alert(url);
																// alert(pathname);
																// alert(hash);
																
																// if element href is equal to current url - hash scroll on current page
																if (url.indexOf(pathname) > -1) {
																	// if has tag exists in dom
																	if (!$('[data-scrolltag="' + hash + '"]').length > 0) {
																		event.preventDefault();
																		linkLocation = this.href;
																		redirectPage();
																		// do this after click
																			jf_preloader_tags.animate({opacity:0});
																			$(".pace").addClass("jf_pace_leave").removeClass("pace-inactive");
																		// alert("leaving");
																	}
																} else {
																	// if hash exists but not in current dom
																		event.preventDefault();
																		linkLocation = this.href;
																		redirectPage();
																		// do this after click
