	{% endfor %}
	
	{% for item_jf_preloader in particle.jf_preloader %}
		{% if item_jf_preloader.enable %}
											{#
											<script src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/jf_preloader/animsition.min.js" type="text/javascript"></script>
											<script type="text/javascript">
												jQuery(document).ready(function($){
													var jf_preloader_wrapper	= '<div id="jf_preloader" class="jf_animsition_overlay_slide"><div class="status"><div class="mdl-spinner mdl-js-spinner is-active is-upgraded" data-upgraded=",MaterialSpinner"></div></div></div>',
														
