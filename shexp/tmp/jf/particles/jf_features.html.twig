{% extends '@nucleus/partials/particle.html.twig' %}

{#

	# 1 - ESCAPE HTML IN JS via TWIG
		{% set html = '<div class="loader"></div>'|e('js') %}
		{% block javascript %}
		<script type="text/javascript">
			jQuery(document).ready(function($){
				$('body').append('{{ html }}');
			});
		</script>
		{% endblock %}

