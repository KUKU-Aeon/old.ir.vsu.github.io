    {#<script type="text/javascript">alert("BOOM");</script>#}
	{% if particle.style == "style_1" %}
		<script type="text/javascript" src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/particles/jf_p_gallery/jquery.stapel.min.js"></script>
		<script type="text/javascript">
			{% set html_loader = '<div class="loader"><i></i><i></i><i></i><i></i><i></i><i></i><span>Loading...</span></div>'|e('js') %}
			{# UNCOMPRESSED
				(function($){	   
					jQuery.fn.extend({
						jf_p_gallery_{{ particle.style_1_id|e }}: function(options) {
							return this.each(function() {
								var jf_p_gallery	= $(this),
									$name 			= jf_p_gallery.prev().find('.cat_name'),
									$close 			= jf_p_gallery.prev().find('.back'),
									$loader			= $('{{ html_loader }}').insertBefore(jf_p_gallery),
									stapel 			= jf_p_gallery.stapel({
										delay : 50,
										gutter : 20,
										onLoad : function() {
											$loader.remove();
											setTimeout(function(){
												if (jf_p_gallery.find('.jf_p_gall_venobox').length){
													$('.jf_p_gall_venobox').venobox({
														numeratio: true,
														infinigall: true
													});
												}
											}, 2000);
										},
										onBeforeOpen : function(pileName){
											$name.html(pileName);
										},
										onAfterOpen : function(pileName){
											$close.animate({'opacity':'1'});
										}
									});
								$close.on('click', function(){
									$close.animate({'opacity':'0'});
									$name.empty();
									stapel.closePile();
								});
							});
						}
					});
				})(jQuery);
			UNCOMPRESSED #}
			!function(n){jQuery.fn.extend({jf_p_gallery_{{ particle.style_1_id|e }}:function(i){return this.each(function(){var i=n(this),e=i.prev().find(".cat_name"),o=i.prev().find(".back"),t=n('{{ html_loader }}').insertBefore(i),a=i.stapel({delay:50,gutter:20,onLoad:function(){t.remove(),setTimeout(function(){i.find(".jf_p_gall_venobox").length&&n(".jf_p_gall_venobox").venobox({numeratio:!0,infinigall:!0})},2e3)},onBeforeOpen:function(n){e.html(n)},onAfterOpen:function(n){o.animate({opacity:"1"})}});o.on("click",function(){o.animate({opacity:"0"}),e.empty(),a.closePile()})})}})}(jQuery);
			jQuery(document).ready(function($){
				$('#{{ particle.style_1_id|e }}').jf_p_gallery_{{ particle.style_1_id|e }}();
			});
		</script>
	{% else %}
		<script type="text/javascript">
			(function($){$(window).load(function(){
				function jf_p_gallery_album() {
					$('.jf_p_gallery_album_thumbs').each(function() {
						var jf_height	= $(this).find('img:nth-child(1)').outerHeight();
						$(this).css({'height':jf_height});
					});
				};
				jf_p_gallery_album();
				$(window).resize(function(){
					jf_p_gallery_album();
				});
			})})(jQuery);
		</script>
	{% endif %}
