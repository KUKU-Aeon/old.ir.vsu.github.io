							<div class="gall_title">
								<div class="name">{{ particle.gall_title|e }}</div>
								<div class="descr">{{ particle.gall_descr|e }}</div>
							</div>
							<div class="cat_name"></div>
							<button id="close" class="back jf_btn fab {{ particle.style_1_back_btn|e }}"><i class="material-icons" style="font-size:30px">arrow_back</i></button>
						</div>
						<ul id="{{ particle.style_1_id|e }}" class="jf_p_gallery">
							{% for item in particle.style_1 %}
								<li data-pile="{{ item.gallery_group|e }}" class="jf_waves_light_40">
									{% if item.linktype == "nolink" %}
										<div>
									{% elseif item.linktype == "simple_link" %}
										<a href="{{ item.simple_link_url|e }}" target="_{{ item.simple_link_target|e }}">
									{% elseif item.linktype == "venobox" %}
