									{% endif %}
									
										{% if item.image_name %}<span class="tp-info"><span>{{ item.image_name|e }}</span></span>{% endif %}
										<img class="lazyload" src="{{ url(particle.style_1_image_lazyload_src)|e }}" data-src="images/{{ item.image[15:] }}"{% if item.image_width %} width="{{ item.image_width|e }}"{% endif %}{% if item.image_height %} height="{{ item.image_height|e }}"{% endif %} alt="{{ item.image_name|e }}" />
									
									{% if item.linktype == "nolink" %}
										</div>
									{% elseif item.linktype == "simple_link" %}
										</a>
									{% elseif item.linktype == "venobox" %}
										</a>
									{% endif %}
								</li>
							{% endfor %}
						</ul>
